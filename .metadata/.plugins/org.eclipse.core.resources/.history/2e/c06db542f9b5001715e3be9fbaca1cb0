package br.com.rich.repository.entity;

import java.time.LocalDateTime;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

	//`A02_NOME`, `A03_LOTE`, `A04_DATA_DE_FABRICACAO`, `A05_DATA_DE_VENCIMENTO`, `A06_QUANTIDADE_POR_EMBALAGEM`, `A07_PRECO`, `A08_STATUS`, 
	//`T24_UNIDADE_DE_MEDIDA_A01_TIPO_DE_MEDIDA`

@Entity
@Table(name="t11_produto")
public class Produto {

	@Column(name = "A01_PRODUTO")
	private int	id;
	@Column(name = "A02_NOME")
	private String	nome;
	@Column(name = "A03_LOTE")
	private String	lote;
	@Column(name = "A04_DATA_DE_FABRICACAO")
	private LocalDateTime	dataFabricacao;
	@Column(name = "A05_DATA_DE_VENCIMENTO")
	private LocalDateTime	dataVencimento;
	
	@Column(name = "A10_DATA_CRIACAO")
	private LocalDateTime	dataCriacao;
	@Column(name = "A11_DATA_MODIFICACAO")
	private LocalDateTime	dataModificacao;
	
}
