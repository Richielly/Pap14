package br.com.rich.repository.entity;

import java.time.LocalDateTime;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

	//`A02_DATA_DO_PEDIDO`, `A03_NUMERO`, `A04_PREVISAO_DE_ENTREGA`, `A05_TIPO_DE_FRETE`, 
	//`A06_OBSERVACOES`, `A07_PRAZOS_DE_VENCIMENTO`, `A08_VALOR_DO_FRETE`, `A09_STATUS`

@Entity
@Table(name="t10_pedido_produto_acabado")
public class PedidoProdutoAcabadoEntity {

	
	@Column(name = "A01_PEDIDO_PRODUTO_ACABADO")
	private LocalDateTime	dataCriacao;
	@Column(name = "A10_DATA_CRIACAO")
	private LocalDateTime	dataCriacao;
	@Column(name = "A11_DATA_MODIFICACAO")
	private LocalDateTime	dataModificacao;
	
	
	
}
